EAAA;AACA,QAAA,KAAA,CAAA,8BAAA,CAAA;AACA,OAFA,MAGA;AACA,aAAA,YAAA;AACA;AACA,KApBA;AAqBA,IAAA,YArBA,0BAqBA;AACA,UAAA,IAAA,GAAA;AACA,QAAA,QAAA,EAAA,KAAA,QADA;AAEA,QAAA,KAAA,EAAA,KAAA,KAFA;AAGA,QAAA,KAAA,EAAA,KAAA,KAHA;AAIA,QAAA,KAAA,EAAA,KAAA,KAJA;AAKA,QAAA,QAAA,EAAA,KAAA;AALA,OAAA;AAOA,MAAA,KAAA,CAAA,IAAA,CAAA,gCAAA,EAAA,IAAA,EACA,IADA,CACA,UAAA,QAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA;AAAA,OADA,EAEA,KAFA,CAEA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OAFA;AAGA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AAjCA;AAdA,CAAA","sourcesContent":["<template>\n  <div class=\"vue-template\">\n    <Header />\n    <div class=\"container inner-block vertical-center\">\n      <form @submit.prevent=\"validate\">\n        <h3>Register</h3>\n\n        <div class=\"form-group\">\n          <label>Username</label>\n          <input v-model=\"username\" type=\"text\" class=\"form-control form-control-lg\">\n          <label v-if=\"this.username.length > 20\">Warning: Exceeded limit</label>\n        </div>\n\n        <div class=\"form-group\">\n          <label>First Name</label>\n          <input v-model=\"fname\" type=\"text\" class=\"form-control form-control-lg\">\n          <label v-if=\"this.fname.length > 20\">Warning: Exceeded limit</label>\n        </div>\n\n        <div class=\"form-group\">\n          <label>Last Name</label>\n          <input v-model=\"lname\" type=\"text\" class=\"form-control form-control-lg\">\n          <label v-if=\"this.lname.length > 20\">Warning: Exceeded limit</label>\n        </div>\n\n        <div class=\"form-group\">\n          <label>Email address</label>\n          <input v-model=\"email\" type=\"email\" class=\"form-control form-control-lg\">\n          <label v-if=\"this.email.length > 40\">Warning: Exceeded limit</label>\n        </div>\n\n        <div class=\"form-group\">\n          <label>Password</label>\n          <input v-model=\"password\" type=\"password\" class=\"form-control form-control-lg\">\n          <label v-if=\"this.password.length > 20\">Warning: Exceeded limit</label>\n        </div>\n\n        <button type=\"submit\" class=\"btn btn-dark btn-lg btn-block\">\n          Register\n        </button>\n\n        <p class=\"forgot-password text-right mt-2 mb-4\">\n          <router-link to=\"instagram.com/?hl=en\">\n            Forgot password?\n          </router-link>\n        </p>\n\n        <div class=\"social-icons\">\n          <ul>\n            <li><a href=\"#\"><i class=\"fa fa-google\" /></a></li>\n            <li><a href=\"#\"><i class=\"fa fa-facebook\" /></a></li>\n            <li><a href=\"#\"><i class=\"fa fa-twitter\" /></a></li>\n          </ul>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport Header from '../components/layout/Header.vue';\n\nexport default {\n  name: 'Register',\n  components: {\n    Header,\n  },\n  data() {\n    return {\n      username: '',\n      fname: '',\n      lname: '',\n      email: '',\n      password: '',\n    };\n  },\n  methods: {\n    validate() {\n      if (this.email.length < 2 || this.email.length > 40) {\n        alert('Email length < 2 or > 20');\n      } \n      else if (this.password.length < 2 || this.password.length > 20) {\n        alert('Password length < 2 or > 20');\n      } \n      else if (this.username.length < 2 || this.username.length > 20) {\n        alert('Username length < 2 or > 20');\n      } \n      else if (this.fname.length < 2 || this.fname.length > 20) {\n        alert('First name length < 2 or > 20');\n      } \n      else if (this.lname.length < 2 || this.lname.length > 20) {\n        alert('Last name length < 2 or > 20');\n      } \n      else {\n        this.handleSubmit();\n      }\n    },\n    handleSubmit() {\n      const data = {\n        username: this.username,\n        fname: this.fname,\n        lname: this.lname,\n        email: this.email,\n        password: this.password,\n      };\n      axios.post('http://localhost:5000/register', data)\n        .then((response) => console.log(response))\n        .catch((err) => console.log(err));\n      this.$router.push('/');\n    },\n  },\n};\n</script>\n\n<style scoped>\n  .container {\n    margin-top: 0.5rem;\n  }\n</style>\n"],"sourceRoot":"src/pages"}]}